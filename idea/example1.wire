// define connectors

X1 [type="D-Sub",
    gender="female",
    pin_labels="DCD|RX|TX|DTR|GND|DSR|RTS|CTS|RI",
   ]

X2 [type="Molex KK 254",
    gender="female",
    pin_labels="GND|RX|TX|NC|OUT|IN",
   ]

// define wire

W1 [mm2=0.25,
    length=0.2,
    num_wires=3,
    colors="din47100",
    shield=true
   ]

// define connections

X1:5 -> W1:1 -> X2:1  // GND
X1:2 -> W1:2 -> X2:3  // TX-RX
X1:3 -> W1:3 -> X2:2  // RX-TX
X1:5 -> W1:S          // shield
X2:5 -> X2:6          // loop
